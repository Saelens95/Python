# A collection of instructions
# A collection of code

def function1():
    print("ahhhh")
    print("ahhhh 2")
print("this is outside the fuction")  

function1()


function1()
function1()

# A mapping
# Input or an arguement

def function2(x):
    return 2*x
    
a = function2(3)
# Return value or output

print(a)

b = function2(4)
print(b)

c = function2(5)
print(c)


def function3(x, y):
    return x + y

e = function3(1, 2)
print(e)


def function4(x):
    print(x)
    print("still in this function")
    return 3*x

f = function4(4)
print(f)


def function5(some_arguement):
    print(some_arguement)
    print("weee")
    
function5(4)
