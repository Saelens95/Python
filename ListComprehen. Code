a = [1, 3, 5, 7, 9, 11]

c = []
for x in a:
    c.append(x * 2)
    print(c)

d = [x * 2 for x in a]
print(d)

#[1, 4 ,9, 16,25, 36]
el = []
for x in range(1, 7):
    el.append(x ** 2)
print(el)

e2 = [x ** 2 for x in range(1, 7)]
print(e2)

for x in range(6, 0, -1):
    print(x)

f1 = []
for x in range(6, 0, -1):
    f1.append(x ** 2)
    print(f1)

f2 = [x ** 2 for x in range(6, 0, -1)]
print(f2)

